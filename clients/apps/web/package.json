{
    "name": "web",
    "version": "1.0.0",
    "type": "module",
    "sideEffects": [
      "*.scss",
      "*.sass"
    ],
    "scripts": {
      "dev": "next dev --turbopack --port 3000",
      "prebuild": "next telemetry disable",
      "build:web": "next build",
      "start:web": "cross-env NODE_ENV=production next start --port 3000",
      "lint": "next lint --max-warnings 0",
      "check-types": "tsc --noEmit"
    },
    "dependencies": {
      "@farm/hooks": "workspace:*",
      "@farm/href": "workspace:*",
      "@farm/models": "workspace:*",
      "@farm/system": "workspace:*",
      "@farm/toolkit": "workspace:*",
      "next": "^15.4.6"
    },
    "devDependencies": {
      "@farm/eslint-config": "workspace:*",
      "@farm/types": "workspace:*",
      "@farm/typescript-config": "workspace:*",
      "sass": "^1.90.0",
      "sass-loader": "^16.0.5"
    }
  }